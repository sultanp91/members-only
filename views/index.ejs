<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <%- include("./partials/header.ejs") %> <%- include("./partials/navbar.ejs")
    %> <% for (const post of posts) { %>
    <h2><%= post.title %></h2>
    <p><%= post.content %></p>

    <% if(user && post.author.member) { %>
    <p>By <%= post.author.username %></p>
    <% } else { %>
    <p>
      By
      <em>Anon</em>
    </p>
    <% } } %>
  </body>
</html>
